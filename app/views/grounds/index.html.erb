  <div id="layout-messages">
    <%= render 'layouts/messages' %>
  </div>
  <div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
      <%= render partial: 'layouts/subdomain_navigation', locals: { type: "ground", path: "grounds_path", new_path: "new_ground_path",
        export_path: "export_grounds_path", import_path: "import_grounds_path"} %>
      <br/>
      <div id="grounds">
        <%= render 'grounds' %>
      </div>
    </div>
  </div>
</div>
