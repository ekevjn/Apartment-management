<%= render 'layouts/sidebarmanager' %>
<%= render 'layouts/topnav' %>
<div class="right_col" role="main" style="min-height: 1575px;">
  <div id="layout-messages">
    <%= render 'layouts/messages' %>
  </div>
  <div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
      <%= form_tag ground_histories_path, :method => 'get',
      :id => "ground_histories_search", class: "form-group" do %>
      <div class="row">
        <div class="col-lg-12">
          <!-- Search box -->
          <div class="input-group">
            <%= text_field_tag :search, params[:search], class: "form-control",
            :placeholder => "Tìm kiếm...", :id => "search_input" %>
            <div class="input-group-btn" data-toggle="tooltip" title="Search">
             <button class="btn btn-default" type="submit">
               <i class="glyphicon glyphicon-search"></i>
             </button>
           </div>
         </div>
         <!-- Search box -->
       </div>
     </div>
     <% end %>
     <br>
     <div id="ground_histories">
      <%= render 'ground_histories' %>
    </div>
  </div>
</div>
</div>
